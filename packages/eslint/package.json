{
  "name": "@youngseo-kangg/eslint-plugin",
  "version": "1.1.0",
  "description": "common configuration for eslint",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "nanobundle build --root-dir=./",
    "prepublishOnly": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Youngseo-kangg/common-config",
    "directory": "packages/eslint"
  },
  "keywords": [
    "eslint",
    "config"
  ],
  "author": "Youngseo Kang <youngseo.kangg@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "source": "./lib/index.js",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "src",
    "lib"
  ],
  "dependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@rushstack/eslint-config": "^4.1.1",
    "@tanstack/eslint-plugin-query": "^5.64.2",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^4.1.4"
  },
  "devDependencies": {
    "nanobundle": "^1.0.0-rc.7",
    "typescript": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
