{
  "name": "@youngseo-kangg/eslint-plugin",
  "version": "1.1.0",
  "description": "common configuration for eslint",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "nanobundle build --root-dir=./",
    "prepublishOnly": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Youngseo-kangg/common-config",
    "directory": "packages/eslint"
  },
  "keywords": [
    "eslint",
    "config"
  ],
  "author": "Youngseo Kang <youngseo.kangg@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "source": "./lib/index.js",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "files": [
    "index.js",
    "src",
    "lib"
  ],
  "dependencies": {
    "@rushstack/eslint-config": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": ">=8.27.0 <9.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "nanobundle": "^1.0.0-rc.7",
    "typescript": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
